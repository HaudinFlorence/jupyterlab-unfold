{
  "name": "@jupyterlab-examples/hello-world-tests",
  "version": "0.1.0",
  "description": "Integration test for hello-world example",
  "repository": "https://github.com/jupyterlab/extension-examples",
  "author": "Project Jupyter Contributors",
  "license": "BSD-3-Clause",
  "private": true,
  "scripts": {
    "install": "playwright install chromium",
    "start-jlab": "jupyter lab --config ./jupyter_server_config.py",
    "start-jlab:detached": "yarn run start-jlab&",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "codegen": "playwright codegen http://localhost:8888/lab"
  },
  "devDependencies": {
    "@playwright/test": "1.14.0",
    "typescript": "~4.1.3"
  }
}
